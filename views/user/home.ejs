<style>
   .btn {
      display: inline-block;
      padding: 12px 20px;
      font-size: 16px;
      font-weight: bold;
      color: #fff;
      background-color: #ff6b6b;
      border: none;
      border-radius: 5px;
      transition: background-color 0.3s, transform 0.3s;
      cursor: pointer;
      text-align: center;
  }


  .btn:hover {
      background-color: #ff4c4c;
      transform: translateY(-2px);
  }


  .btn:active {
      transform: translateY(1px);
  }
  .wishlist-btn {
      position: absolute;
      top: 10px;
      right: 10px;
      background-color: rgba(255, 255, 255, 0.8);
      padding: 10px;
      border-radius: 50%;
      color: #ff6b6b;
      font-size: 18px;
      z-index: 10;
      transition: background-color 0.3s, color 0.3s, transform 0.3s;
  }


  .wishlist-btn:hover {
      background-color: rgba(255, 255, 255, 1);
      color: #ff4c4c;
      transform: translateY(-2px);
  }


  .wishlist-btn:active {
      transform: translateY(1px);
  }


  .product-img-action-wrap {
      position: relative;
  }


  .product-img {
      position: relative;
      display: block;
      overflow: hidden;
  }


  .product-cart-wrap {
      border: 1px solid #eee;
      padding: 20px;
      margin-bottom: 30px;
  }


  .product-content-wrap h6 {
      font-size: 18px;
      color: #333;
      margin-bottom: 5px;
  }


  .product-content-wrap h5 {
      font-size: 16px;
      color: #ff6b6b;
      margin-bottom: 10px;
  }


  .product-content-wrap a {
      display: inline-block;
      padding: 8px 12px;
      background-color: #ff6b6b;
      color: white;
      text-transform: uppercase;
      font-size: 14px;
      border-radius: 4px;
      text-decoration: none;
  }


  .product-content-wrap a:hover {
      background-color: #ff4c4c;
  }

  </style> 




  <style>
  .filter__controls {
    display: flex;
    justify-content: center;
    margin-bottom: 30px;
  }

  .filter__controls li {
    list-style: none;
    padding: 10px 20px;
    font-size: 18px;
    font-weight: bold;
    cursor: pointer;
    background-color: #ff6b6b;
    color: #fff;
    border-radius: 5px;
    transition: background-color 0.3s;
    margin: 0 10px;
  }

  .filter__controls li:hover {
    background-color: #ff4c4c;
  }

  .product__filter {
    display: flex;
    flex-wrap: wrap;
    gap: 30px;
    justify-content: center;
  }

  .product-cart-wrap {
    position: relative;
    width: 100%;
    max-width: 250px;
    background-color: #fff;
    border-radius: 8px;
    box-shadow: 0 0 15px rgba(0, 0, 0, 0.1);
    overflow: hidden;
    transition: transform 0.3s;
  }

  .product-cart-wrap:hover {
    transform: scale(1.05);
  }

  .product-img {
    display: block;
    width: 100%;
    height: 200px;
    object-fit: cover;
  }

  .product-action-1 {
    position: absolute;
    top: 10px;
    right: 10px;
    background-color: rgba(255, 255, 255, 0.6);
    border-radius: 50%;
    padding: 8px;
    color: #ff6b6b;
    font-size: 18px;
    transition: background-color 0.3s, color 0.3s;
  }

  .product-action-1:hover {
    background-color: rgba(255, 255, 255, 1);
    color: #ff4c4c;
  }

  .product-content-wrap {
    padding: 15px;
    text-align: center;
  }

  .product-content-wrap h6 {
    font-size: 16px;
    color: #333;
    margin-bottom: 8px;
  }

  .product-content-wrap h5 {
    font-size: 18px;
    color: #ff6b6b;
    margin-bottom: 12px;
  }

  .product-content-wrap a {
    display: inline-block;
    padding: 10px 15px;
    background-color: #ff6b6b;
    color: white;
    font-size: 14px;
    text-transform: uppercase;
    text-decoration: none;
    border-radius: 4px;
    transition: background-color 0.3s;
  }

  .product-content-wrap a:hover {
    background-color: #ff4c4c;
  }
</style>




